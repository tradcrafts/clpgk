     (LET ((NewResult (lisp-form V334 (CAR (CDR V336)))))
      (CONS (LIST (CONS (CAR V337) NewTests) NewResult)
       (make-lispform V334 (CDR V335)))))))
  ((AND (CONSP V335) (CONSP (CAR V335)) (CONSP (CDR (CAR V335)))
    (NULL (CDR (CDR (CAR V335)))))
