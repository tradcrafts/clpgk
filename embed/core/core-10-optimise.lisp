(DEFUN reachable? (V171 V172)
 (COND
  ((AND (CONSP V172) (CONSP (CDR V172)) (NULL (CDR (CDR V172)))
    (AND (ABSEQUAL V171 (CAR (CDR V172))) (EQ 'GO (CAR V172))))
   'true)
  ((CONSP V172)
   (THE SYMBOL (or (reachable? V171 (CAR V172)) (reachable? V171 (CDR V172)))))
  (T 'false)))

