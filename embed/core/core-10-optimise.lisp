   (LET* ((V17 (CDR V16)) (V18 (CDR V17)) (V19 (CDR V18)) (V20 (CAR V19)))
    (LIST (CAR V16) (CAR V17) (CAR V18)
     (CONS (CAR V20) (THE LIST (MAPCAR 'occ (CDR V20)))))))
