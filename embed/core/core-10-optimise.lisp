  ((EQL 0 V24)
   (LET ((B (find-assoc-type V25)))
    (IF (NULL B) V26 (LIST 'THE B V26))))
  ((AND (CONSP V25) (CONSP (CDR V25)) (EQ '--> (CAR (CDR V25)))
    (CONSP (CDR (CDR V25))) (NULL (CDR (CDR (CDR V25)))))
